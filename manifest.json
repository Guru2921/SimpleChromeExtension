{
  "manifest_version" : 2,
  "name" : "User call Status",
  "description" : "Change icon depends on number of calls",
  "version": "1.0.0",
  "minimum_chrome_version": "46",
  "icons": {
    "128": "icons/icon_white.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_white.png",
    "default_popup" : "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery-1.8.1.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}

